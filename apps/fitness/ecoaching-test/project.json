{
  "name": "fitness-ecoaching-test",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo Build succeeded."
      }
    },
    "watch": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx run-many --targets=build --projects=tag:firebase:dep:fitness-ecoaching-test --parallel=100 --watch"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx run-many --targets=lint --projects=tag:firebase:dep:fitness-ecoaching-test --parallel=100"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx run-many --targets=test --projects=tag:firebase:dep:fitness-ecoaching-test --parallel=100"
      }
    },
    "firebase": {
      "executor": "nx:run-commands",
      "options": {
        "command": "firebase --config=firebase.json --project=fitness"
      },
      "configurations": {
        "production": {
          "command": "firebase --config=firebase.json --project=fitness"
        }
      }
    },
    "killports": {
      "executor": "nx:run-commands",
      "options": {
        "command": "kill-port --port 9099,5001,8080,9000,5000,8085,9199,9299,4000,4400,4500"
      }
    },
    "getconfig": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx run fitness-ecoaching-test:firebase functions:config:get > apps/fitness/ecoaching-test/.runtimeconfig.json"
      }
    },
    "emulate": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx run fitness-ecoaching-test:killports",
          "nx run fitness-ecoaching-test:firebase emulators:start --import=apps/fitness/ecoaching-test/.emulators --export-on-exit"
        ],
        "parallel": false
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx run fitness-ecoaching-test:watch",
          "nx run fitness-ecoaching-test:emulate"
        ]
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "dependsOn": [
        "build"
      ],
      "options": {
        "command": "nx run fitness-ecoaching-test:firebase deploy"
      }
    }
  },
  "tags": [
    "firebase:app",
    "firebase:name:fitness-ecoaching-test"
  ]
}
